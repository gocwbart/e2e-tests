{
  "name": "pulse-admin-v2",
  "version": "0.0.1",
  "description": "Pulse Admin v2",
  "main": "server/app.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run _server:run\" \"ng serve --aot=true --progress=false --proxy-config proxy.conf.json\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test",
    "e2e:client": "ng e2e",
    "build": "ng build --prod --sm=false --aot=true --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "author": "Balboa Features",
  "private": true,
  "engines": {
    "node": "6.10.x",
    "npm": "3.10.x"
  },
  "dependencies": {
    "@angular-mdl/core": "^4.0.6",
    "@angular/cli": "^1.0.0",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@swimlane/ngx-datatable": "^9.2.0",
    "@types/body-parser": "^1.16.3",
    "@types/compression": "^0.0.33",
    "@types/express": "^4.0.35",
    "@types/jasmine": "^2.5.49",
    "@types/node": "^7.0.23",
    "body-parser": "~1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "ng2-smart-table": "^1.2.1",
    "path": "^0.12.7",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.2",
    "ts-node": "~3.0.2",
    "tslint": "^5.4.2",
    "typescript": "^2.2.2",
    "uuid": "^3.0.1",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "codelyzer": "^3.0.1",
    "concurrently": "^3.1.0",
    "jasmine-core": "^2.6.2",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "webdriver-manager": "^12.0.6"
  }
}
