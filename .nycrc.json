{
  "report-dir": "./coverage/server",
  "check-coverage": true,
  "lines": 10,
  "statements": 10,
  "functions": 10,
  "branches": 0,
  "include": [
    "server/**/*.ts"
  ],
  "exclude": [
    "server/bin",
    "server/config.ts",
    "server/**/*.spec.ts"
  ],
  "reporter": [
    "html",
    "text-summary"
  ],
  "extension": [
    ".ts"
  ],
  "cache": true,
  "all": true
}
